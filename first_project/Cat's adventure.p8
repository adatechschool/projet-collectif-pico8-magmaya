--map

function draw_map()
	map()
end

function check_flag(flag,x,y)
	local sprite = mget(x,y)
	return fget(sprite,flag)
end


function update_camera()
 camx=mid(0,p.x-7.5+p.ox/8,127-15)
 camy=mid(0,p.y-7.5+p.oy/8,31-15)
 camera(camx*8,camy*8)
end

function object_taken(x,y)
	sprite=mget(x,y)
	if x==53 and y==5 then
		sprite=mget(x,y)
		mset(x,y,25)
		p.hearts+=3
	else
	mset(x,y,25)
	p.hearts+=1
	end
end


function pick_up(x,y)
 object_taken(x,y)
end

	





 
